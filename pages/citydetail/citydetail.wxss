/* pages/citydetail/citydetail.wxss */
.citydetail-wrap{

}
.citydetail-top{
  width:100%;
  height:110rpx;
  background: #0C1930;
  display: flex;
  align-items: center;
  padding: 0 25rpx 0 17rpx;
  box-sizing: border-box;
  justify-content: space-between;
}
.top-logo{
  width:208rpx;
  height:70rpx;
}

.input-wrap {
  width: 475rpx;
  height: 70rpx;
  background: #FFFFFF;
  border-radius: 15rpx;
  display: flex;
  align-items: center;
}

.input-style {
  width: 326rpx;
  height: 70rpx;
  padding-left: 20rpx;
  font-size: 30rpx;
}

.input-close {
  width: 35rpx;
  height: 35rpx;
  margin-right: 14rpx;
}

.input-search {
  width: 80rpx;
  height: 70rpx;
  background: #4D93B4;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 0 15rpx 15rpx 0;
}

.search-img {
  width: 35rpx;
  height: 35rpx;
}
.position-wrap{
  height:80rpx;
  padding: 0 26rpx;
  font-size: 30rpx;
  color:#146092;
display: flex;
align-items: center;
}
.position-img{
  width:41rpx;
  height:39rpx;
  margin-right:20rpx;
}